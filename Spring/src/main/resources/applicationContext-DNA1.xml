<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--    <bean id="userDao" class="com.DNA1.dao.Impl.UserDaoImpl" init-method="init" destroy-method="destroy" ></bean>-->
    <!--    <bean id="userDao" class="com.DNA1.factory.StaticFactory" factory-method="getUserDao"></bean>-->
    <!--    <bean id="factory" class="com.DNA1.factory.DynamicFactory"></bean>-->
    <!--    <bean id="userDao" factory-bean="factory" factory-method="getUserDao"></bean>-->
    <bean id="user" class="com.DNA1.domain.User">
        <property name="username" value="DNA"></property>
        <property name="age" value="18"></property>
    </bean>
    <bean id="userDao" class="com.DNA1.dao.Impl.UserDaoImpl">
        <!--        <property name="username" value="DNA"></property>-->
        <!--        <property name="age" value="18"></property>-->
        <property name="strList">
            <list>
                <value>aaa</value>
                <value>bbb</value>
                <value>ccc</value>
            </list>
        </property>
        <property name="userMap">
            <map>
                <entry key="user1" value-ref="user"></entry>
                <entry key="user2" value-ref="user"></entry>
            </map>
        </property>
        <property name="properties">
            <props>
                <prop key="p1">ppp</prop>
                <prop key="p2"></prop>
            </props>
        </property>
    </bean>
    <!--    &lt;!&ndash;  依赖注入  &ndash;&gt;-->
    <!--    <bean id="userService" class="com.DNA1.service.impl.UserServiceImpl">-->
    <!--        <property name="userDao" ref="userDao"></property>-->
    <!--    </bean>-->
    <!--    <bean - id="userService" class="com.DNA1.service.impl.UserServiceImpl" p:userDao-ref="userDao"></bean>-->
    <bean id="userService" class="com.DNA1.service.impl.UserServiceImpl">
        <constructor-arg name="userDao" ref="userDao"></constructor-arg>
    </bean>
</beans>